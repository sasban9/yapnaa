			$('#subs_name').focus();
			$('#loader-image').hide();
			return false;
			}
			else if(subs_email == ''){
			$('#err_subs_message').html('<p style="color:red">Email required</p>');
			$('#subs_email').focus();
			$('#loader-image').hide();
			return false;
			}
			else if(!re.test(subs_email)){
